Imports System
Imports System.Diagnostics.Metrics

Module Program
    Sub Main(args As String())
        Dim i, Counter As Integer
        Counter = 0
        For i = Counter To 10 Step 1
            Console.WriteLine("This is a line")
        Next Counter /

        For Counter = 10 To 1 Step -1
            Console.WriteLine(Counter)
        Next Counter
    End Sub
End Module
